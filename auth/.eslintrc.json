{
    "extends": "airbnb",
    "plugins": ["prettier"],
    "rules": {
        "react/jsx-filename-extension": 0,
        "react/prefer-stateless-function": "off",
        "import/prefer-default-export": "off",
        "no-use-before-define": "off",
        "prettier/prettier": "error",
        "singleQuote": true
    }
}